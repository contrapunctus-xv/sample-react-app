    "react-shallow-testutils": "^2.0.0",
    "render-shallow": "^1.0.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^1.13.1",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-dev-server": "^1.12.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gonebusy/sample-react-app.git"
